{
    "order": 1,
    "index_patterns": [
        "mds-nginx-access-*"
    ],
    "settings": {
        "index": {
            "mapping": {
                "total_fields": {
                    "limit": "10000"
                }
            },
            "refresh_interval": "5s",
            "number_of_routing_shards": "30",
            "number_of_shards": "3"
        }
    },
    "mappings": {
        "doc": {
            "doc": {
                "dynamic_templates": [
                    {
                        "fields": {
                            "path_match": "fields.*",
                            "mapping": {
                                "type": "keyword"
                            },
                            "match_mapping_type": "string"
                        }
                    },
                    {
                        "strings_as_keyword": {
                            "match_mapping_type": "string",
                            "mapping": {
                                "ignore_above": 1024,
                                "type": "keyword"
                            }
                        }
                    }
                ],
                "properties": {
                    "@timestamp": {
                        "type": "date"
                    },
                    "@version": {
                        "type": "keyword",
                        "ignore_above": 1024
                    },
                    "data": {
                        "properties": {
                            "document_manager_guid": {
                                "type": "keyword",
                                "ignore_above": 1024
                            },
                            "filename": {
                                "type": "keyword",
                                "ignore_above": 1024
                            },
                            "latitude": {
                                "type": "keyword",
                                "ignore_above": 1024
                            },
                            "longitude": {
                                "type": "keyword",
                                "ignore_above": 1024
                            },
                            "mine_guid": {
                                "type": "keyword",
                                "ignore_above": 1024
                            },
                            "mine_region": {
                                "type": "keyword",
                                "ignore_above": 1024
                            },
                            "mine_status": {
                                "type": "keyword",
                                "ignore_above": 1024
                            },
                            "name": {
                                "type": "keyword",
                                "ignore_above": 1024
                            },
                            "note": {
                                "type": "keyword",
                                "ignore_above": 1024
                            },
                            "tsf_name": {
                                "type": "keyword",
                                "ignore_above": 1024
                            }
                        }
                    },
                    "host": {
                        "type": "keyword",
                        "ignore_above": 1024
                    },
                    "jwt_data": {
                        "properties": {
                            "acr": {
                                "type": "keyword",
                                "ignore_above": 1024
                            },
                            "allowed-origins": {
                                "type": "keyword",
                                "ignore_above": 1024
                            },
                            "aud": {
                                "type": "keyword",
                                "ignore_above": 1024
                            },
                            "auth_time": {
                                "type": "long"
                            },
                            "azp": {
                                "type": "keyword",
                                "ignore_above": 1024
                            },
                            "email": {
                                "type": "keyword",
                                "ignore_above": 1024
                            },
                            "exp": {
                                "type": "long"
                            },
                            "iat": {
                                "type": "long"
                            },
                            "iss": {
                                "type": "keyword",
                                "ignore_above": 1024
                            },
                            "jti": {
                                "type": "keyword",
                                "ignore_above": 1024
                            },
                            "nbf": {
                                "type": "long"
                            },
                            "nonce": {
                                "type": "keyword",
                                "ignore_above": 1024
                            },
                            "preferred_username": {
                                "type": "keyword",
                                "ignore_above": 1024
                            },
                            "realm_access": {
                                "properties": {
                                    "roles": {
                                        "type": "keyword",
                                        "ignore_above": 1024
                                    }
                                }
                            },
                            "resource_access": {
                                "properties": {
                                    "account": {
                                        "properties": {
                                            "roles": {
                                                "type": "keyword",
                                                "ignore_above": 1024
                                            }
                                        }
                                    }
                                }
                            },
                            "session_state": {
                                "type": "keyword",
                                "ignore_above": 1024
                            },
                            "sub": {
                                "type": "keyword",
                                "ignore_above": 1024
                            },
                            "typ": {
                                "type": "keyword",
                                "ignore_above": 1024
                            }
                        }
                    },
                    "message": {
                        "type": "text",
                        "norms": false
                    },
                    "log": {
                        "properties": {
                            "type": {
                                "type": "keyword",
                                "ignore_above": 1024
                            }
                        }
                    },
                    "mine": {
                        "properties": {
                            "id": {
                                "type": "keyword",
                                "ignore_above": 1024
                            }
                        }
                    },
                    "mine_data": {
                        "properties": {
                            "location": {
                                "type": "geo_point"
                            },
                            "name": {
                                "type": "keyword",
                                "ignore_above": 1024
                            },
                            "numero": {
                                "type": "keyword",
                                "ignore_above": 1024
                            },
                            "region": {
                                "type": "keyword",
                                "ignore_above": 1024
                            }
                        }
                    },
                    "mds": {
                        "properties": {
                            "group": {
                                "type": "keyword",
                                "ignore_above": 1024
                            }
                        }
                    },
                    "nginx": {
                        "properties": {
                            "access": {
                                "properties": {
                                    "agent": {
                                        "type": "text",
                                        "norms": false
                                    },
                                    "body_sent": {
                                        "properties": {
                                            "bytes": {
                                                "type": "long"
                                            }
                                        }
                                    },
                                    "client_ip": {
                                        "type": "keyword",
                                        "ignore_above": 1024
                                    },
                                    "geoip": {
                                        "properties": {
                                            "city_name": {
                                                "type": "keyword",
                                                "ignore_above": 1024
                                            },
                                            "continent_code": {
                                                "type": "keyword",
                                                "ignore_above": 1024
                                            },
                                            "continent_name": {
                                                "type": "keyword",
                                                "ignore_above": 1024
                                            },
                                            "country_code2": {
                                                "type": "keyword",
                                                "ignore_above": 1024
                                            },
                                            "country_code3": {
                                                "type": "keyword",
                                                "ignore_above": 1024
                                            },
                                            "country_iso_code": {
                                                "type": "keyword",
                                                "ignore_above": 1024
                                            },
                                            "country_name": {
                                                "type": "keyword",
                                                "ignore_above": 1024
                                            },
                                            "ip": {
                                                "type": "keyword",
                                                "ignore_above": 1024
                                            },
                                            "latitude": {
                                                "type": "float"
                                            },
                                            "location": {
                                                "type": "geo_point"
                                            },
                                            "longitude": {
                                                "type": "float"
                                            },
                                            "region_iso_code": {
                                                "type": "keyword",
                                                "ignore_above": 1024
                                            },
                                            "region_name": {
                                                "type": "keyword",
                                                "ignore_above": 1024
                                            }
                                        }
                                    },
                                    "http_version": {
                                        "type": "keyword",
                                        "ignore_above": 1024
                                    },
                                    "method": {
                                        "type": "keyword",
                                        "ignore_above": 1024
                                    },
                                    "payload": {
                                        "type": "keyword",
                                        "ignore_above": 1024
                                    },
                                    "referrer": {
                                        "type": "keyword",
                                        "ignore_above": 1024
                                    },
                                    "remote_ip": {
                                        "type": "keyword",
                                        "ignore_above": 1024
                                    },
                                    "response_code": {
                                        "type": "long"
                                    },
                                    "server": {
                                        "properties": {
                                            "geoip": {
                                                "properties": {
                                                    "city_name": {
                                                        "type": "keyword",
                                                        "ignore_above": 1024
                                                    },
                                                    "continent_code": {
                                                        "type": "keyword",
                                                        "ignore_above": 1024
                                                    },
                                                    "country_code2": {
                                                        "type": "keyword",
                                                        "ignore_above": 1024
                                                    },
                                                    "country_code3": {
                                                        "type": "keyword",
                                                        "ignore_above": 1024
                                                    },
                                                    "country_name": {
                                                        "type": "keyword",
                                                        "ignore_above": 1024
                                                    },
                                                    "ip": {
                                                        "type": "keyword",
                                                        "ignore_above": 1024
                                                    },
                                                    "latitude": {
                                                        "type": "float"
                                                    },
                                                    "location": {
                                                        "properties": {
                                                            "lat": {
                                                                "type": "float"
                                                            },
                                                            "lon": {
                                                                "type": "float"
                                                            }
                                                        }
                                                    },
                                                    "longitude": {
                                                        "type": "float"
                                                    },
                                                    "postal_code": {
                                                        "type": "keyword",
                                                        "ignore_above": 1024
                                                    },
                                                    "region_code": {
                                                        "type": "keyword",
                                                        "ignore_above": 1024
                                                    },
                                                    "region_name": {
                                                        "type": "keyword",
                                                        "ignore_above": 1024
                                                    },
                                                    "timezone": {
                                                        "type": "keyword",
                                                        "ignore_above": 1024
                                                    }
                                                }
                                            }
                                        }
                                    },
                                    "url": {
                                        "type": "keyword",
                                        "ignore_above": 1024
                                    },
                                    "user_agent": {
                                        "properties": {
                                            "build": {
                                                "type": "keyword",
                                                "ignore_above": 1024
                                            },
                                            "device": {
                                                "type": "keyword",
                                                "ignore_above": 1024
                                            },
                                            "major": {
                                                "type": "long"
                                            },
                                            "minor": {
                                                "type": "long"
                                            },
                                            "name": {
                                                "type": "keyword",
                                                "ignore_above": 1024
                                            },
                                            "original": {
                                                "type": "text",
                                                "index": false,
                                                "norms": false
                                            },
                                            "os": {
                                                "type": "keyword",
                                                "ignore_above": 1024
                                            },
                                            "os_major": {
                                                "type": "long"
                                            },
                                            "os_minor": {
                                                "type": "long"
                                            },
                                            "os_name": {
                                                "type": "keyword",
                                                "ignore_above": 1024
                                            },
                                            "patch": {
                                                "type": "keyword",
                                                "ignore_above": 1024
                                            }
                                        }
                                    },
                                    "user_name": {
                                        "type": "keyword",
                                        "ignore_above": 1024
                                    }
                                }
                            },
                            "error": {
                                "properties": {
                                    "connection_id": {
                                        "type": "long"
                                    },
                                    "level": {
                                        "type": "keyword",
                                        "ignore_above": 1024
                                    },
                                    "message": {
                                        "type": "text",
                                        "norms": false
                                    },
                                    "pid": {
                                        "type": "long"
                                    },
                                    "tid": {
                                        "type": "long"
                                    }
                                }
                            }
                        }
                    },
                    "operation": {
                        "type": "keyword",
                        "ignore_above": 1024
                    },
                    "path": {
                        "type": "text"
                    },
                    "read_timestamp": {
                        "type": "date"
                    },
                    "tags": {
                        "type": "keyword",
                        "ignore_above": 1024
                    }
                }
            }
        }
    }
}