timezone: UTC

schedule:
  daily>: 1:00:00   # daily at 6 PM PDT

+setup:
  echo>: Starting IDIR ETL ${session_time}

+login:
  sh>: oc login https://console.pathfinder.gov.bc.ca:8443 --token=$OC_TOKEN

+run_etl_job:
  py>: tasks.idir_users_import.run_job

+teardown:
  echo>: finish ${session_time}
