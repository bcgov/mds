server.bind=0.0.0.0
server.port=8080
server.admin.bind=0.0.0.0
server.admin.port=8081
server.http.enable-http2=true
server.authenticator-class=io.digdag.standards.auth.basic.BasicAuthenticator
basicauth.username=${DIGDAG_USER_NAME}
basicauth.password=${DIGDAG_USER_PASSWORD}
basicauth.admin=true
digdag.secret-encryption-key=${DIGDAG_ENCRYPTION_KEY}
log-server.type=local