{
  "kind": "Template",
  "apiVersion": "v1",
  "metadata": {
    "name": "mds-python-oracle-bc",
    "creationTimestamp": null
  },
  "parameters": [
    {
      "name": "SUFFIX",
      "displayName": "Name Suffix",
      "description": "A suffix appended to all objects",
      "required": true
    },
    {
      "name": "VERSION",
      "required": true
    }
  ],
  "objects": [
    {
      "kind": "ImageStream",
      "apiVersion": "v1",
      "metadata": {
        "name": "oracle-instant-client",
        "creationTimestamp": null,
        "annotations": {
          "description": "Oracle Instant Client"
        },
        "labels": {
          "shared": "true"
        }
      },
      "spec": {
        "lookupPolicy": {
          "local": false
        },
        "tags": [
          {
            "name": "7-slim",
            "annotations": null,
            "from": {
              "kind": "DockerImage",
              "name": "oraclelinux:7-slim"
            },
            "importPolicy": {},
            "referencePolicy": {
              "type": "Local"
            }
          }
        ]
      }
    },
    {
      "kind": "ImageStream",
      "apiVersion": "v1",
      "metadata": {
        "name": "mds-oracle-instant-client",
        "creationTimestamp": null,
        "annotations": {
          "description": "Python Runtime Image"
        },
        "labels": {
          "shared": "true"
        }
      },
      "spec": {
        "lookupPolicy": {
          "local": false
        }
      }
    },
    {
      "kind": "BuildConfig",
      "apiVersion": "v1",
      "metadata": {
        "name": "mds-oracle-instant-client${SUFFIX}",
        "creationTimestamp": null,
        "labels": {},
        "annotations": {}
      },
      "spec": {
        "completionDeadlineSeconds": 1440,
        "successfulBuildsHistoryLimit": 3,
        "failedBuildsHistoryLimit": 3,
        "triggers": [
          {
            "type": "ImageChange",
            "imageChange": {}
          },
          {
            "type": "ConfigChange"
          }
        ],
        "runPolicy": "SerialLatestOnly",
        "source": {
          "contextDir": "services/nris-api-test",
          "type": "Binary"
        },
        "strategy": {
          "type": "Docker",
          "dockerStrategy": {
            "from": {
              "kind": "ImageStreamTag",
              "name": "oracle-instant-client:7-slim"
            },
            "env": []
          }
        },
        "output": {
          "to": {
            "kind": "ImageStreamTag",
            "name": "mds-oracle-instant-client:${VERSION}"
          }
        },
        "resources": {
          "limits": {
            "cpu": "500m",
            "memory": "2Gi"
          },
          "requests": {
            "cpu": "250m",
            "memory": "1Gi"
          }
        },
        "postCommit": {},
        "nodeSelector": null
      }
    }
  ]
}
