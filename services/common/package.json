{
  "name": "@mds/common",
  "version": "0.1.0",
  "description": "Common library for MDS Frontend",
  "private": true,
  "homepage": "${BASE_PATH}/",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "build": "webpack --env production",
    "watch": "webpack --watch --env development",
    "test": "jest --env=jsdom",
    "ci-test": "jest --env=jsdom --testResultsProcessor=jest-sonar-reporter --update-snapshot --runInBand --coverage",
    "test:updateAll": "jest --env=jsdom -u --"
  },
  "dependencies": {
    "@reduxjs/toolkit": "2.0.1",
    "flagsmith": "3.19.1",
    "p-limit": "3.1.0",
    "query-string": "5.1.1",
    "ts-loader": "8.4.0"
  },
  "peerDependencies": {
    "@types/react-router-dom": "5.3.3",
    "antd": "4.24.10",
    "react": "16.13.1",
    "react-dom": "16.13.1",
    "react-router-dom": "5.3.3"
  },
  "devDependencies": {
    "clean-webpack-plugin": "4.0.0",
    "copy-webpack-plugin": "10.1.0",
    "hard-source-webpack-plugin": "0.13.1",
    "html-webpack-plugin": "5.5.3",
    "image-minimizer-webpack-plugin": "3.8.3",
    "imagemin": "8.0.1",
    "jest": "29.7.0",
    "mini-css-extract-plugin": "2.7.6",
    "postcss-loader": "7.3.3",
    "prettier": "1.19.1",
    "react-hot-loader": "4.12.21",
    "sharp": "^0.33.3",
    "style-loader": "3.3.3",
    "terser-webpack-plugin": "5.3.9",
    "ts-jest": "29.1.2",
    "typescript": "4.7.4",
    "webpack": "5.88.1",
    "webpack-cli": "5.1.4",
    "webpack-dev-server": "4.15.1",
    "webpack-manifest-plugin": "5.0.0",
    "webpack-merge": "5.9.0",
    "webpack-node-externals": "3.0.0"
  },
  "optionalDependencies": {
    "jest-canvas-mock": "2.1.0",
    "jest-environment-jsdom": "29.7.0",
    "jest-environment-jsdom-global": "4.0.0",
    "jest-junit": "6.4.0",
    "jest-sonar-reporter": "2.0.0"
  },
  "jestSonar": {
    "reportPath": "reports",
    "reportFile": "test-reporter.xml",
    "indent": 4
  },
  "jest-junit": {
    "output": "./coverage/tests-report.xml"
  }
}
