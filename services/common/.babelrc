{
  "presets": [
    [
      "@babel/preset-env",
      {
        "useBuiltIns": "entry",
        "corejs": 3,
        "targets": { "node": "current", "ie": "8" }
      }
    ],
    ["@babel/preset-react"]
  ],
  "plugins": [
    [
      "@babel/transform-runtime",
      {
        "corejs": 3
      }
    ],
    ["@babel/plugin-proposal-class-properties"],
    ["react-hot-loader/babel"]
  ]
}
