  "scripts": {
    "serve": "cross-env HOST=0.0.0.0 PORT=3020 webpack-dev-server --hot --env development",
    "build": "webpack --env production",
    "production": "node server.js",
    "verify": "webpack --env production configtest webpack.config.ts",
    "ci-test": "CI=true jest --env=jsdom --testResultsProcessor=jest-sonar-reporter --coverage",
    "lint": "eslint ./src",
    "lint:file": "eslint",
    "format:check": "prettier",
    "format:check-all": "prettier --config ../../.prettierrc.json src/*.{js,ts,jsx,tsx} && prettier src/**/*.{js,ts,jsx,tsx} && prettier --config .prettierrc.json src/**/**/*.{js,ts,jsx,tsx}",
    "format:write": "prettier --config ../../.prettierrc.json src/*.js --write",
    "format:write-all": "prettier --config ../../.prettierrc.json src/*.{js,ts,jsx,tsx} --write && prettier --config ../../.prettierrc.json src/**/*.{js,ts,jsx,tsx} --write && prettier --config ../../.prettierrc.json src/**/**/*.{js,ts,jsx,tsx} --write",
    "test": "jest --env=jsdom",
    "test:coverage": "jest --env=jsdom --runInBand src -- --testResultsProcessor=jest-junit --coverage",
    "test:updateAll": "jest --env=jsdom --runInBand -u --",
    "test:watch": "jest --env=jsdom --runInBand -- src --watch",
    "test:only": "jest"
  },
  "jestSonar": {
    "reportPath": "reports",
    "reportFile": "test-reporter.xml",
    "indent": 4
  },
  "jest-junit": {
    "output": "./coverage/tests-report.xml"
  },
  "browserslist": [
    "> 0.1%"
  ],
  "lint-staged": {
    "*.{js,ts,tsx,css,json,md}": [
      "yarn format:write",
      "git add"
    ],
    "../minespace-web/**/*.{js,ts,tsx,css,json,md}": [
      "yarn format:write",
      "git add"
    ],
    "*.js,*.ts,*.tsx": [
      "eslint --fix",
      "git add"
    ],
    "../minespace-web/**/*.{js,ts,tsx}": [
      "eslint --fix",
      "git add"
    ]
  },
  "lint-prepush": {
    "base": "develop",
    "tasks": {
      "*.{js,ts,tsx}": []
    }
  }
}
