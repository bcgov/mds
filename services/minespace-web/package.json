    "jest-environment-jsdom-global": "1.2.0",
    "jest-junit": "6.4.0",
    "jest-localstorage-mock": "2.4.0",
    "jest-sonar-reporter": "2.0.0"
  },
  "scripts": {
    "serve": "cross-env HOST=0.0.0.0 PORT=3020 webpack-dev-server --hot --env development",
    "build": "webpack --env production",
    "production": "node server.js",
    "verify": "webpack --env production configtest webpack.config.ts",
    "ci-test": "CI=true jest --env=jsdom --testResultsProcessor=jest-sonar-reporter --update-snapshot --runInBand --coverage",
    "lint": "eslint ./src",
    "lint:file": "eslint",
    "format:check": "prettier",
    "format:check-all": "prettier --config ../../.prettierrc.json src/*.{js,ts,jsx,tsx} && prettier src/**/*.{js,ts,jsx,tsx} && prettier --config .prettierrc.json src/**/**/*.{js,ts,jsx,tsx}",
    "format:write": "prettier --config ../../.prettierrc.json src/*.js --write",
    "format:write-all": "prettier --config ../../.prettierrc.json src/*.{js,ts,jsx,tsx} --write && prettier --config ../../.prettierrc.json src/**/*.{js,ts,jsx,tsx} --write && prettier --config ../../.prettierrc.json src/**/**/*.{js,ts,jsx,tsx} --write",
    "test": "jest --env=jsdom --runInBand",
    "test:coverage": "jest --env=jsdom --runInBand src -- --testResultsProcessor=jest-junit --coverage",
    "test:updateAll": "jest --env=jsdom --runInBand -u --",
    "test:watch": "jest --env=jsdom --runInBand -- src --watch",
    "test:only": "jest"
  },
  "jest": {
    "transform": {
      "^.+\\.jsx?$": "babel-jest",
      "^.+\\.tsx?$": "ts-jest"
    },
    "globals": {
      "ts-jest": {
        "tsConfigFile": "tsconfig.json"
      }
    },
    "verbose": true,
    "testURL": "http://localhost",
    "testEnvironment": "jest-environment-jsdom-global",
    "setupFiles": [
      "./src/setupTests.js",
      "jest-localstorage-mock",
      "jest-canvas-mock"
    ],
    "collectCoverageFrom": [
      "!**/common/**/*.{js,ts,tsx}",
      "**/src/**/*.{js,ts,tsx}",
      "!src/index.{js,ts,tsx}",
      "!src/App.{js,ts,tsx}",
      "!src/fetchEnv.{js,ts,tsx}",
      "!src/assetsTransformer.{js,ts,tsx}",
      "!src/setupTests.{js,ts,tsx}"
    ],
    "moduleNameMapper": {
      "^@/(.*)$": "<rootDir>/src/$1",
      "^@common/(.*)$": "<rootDir>/common/$1",
      "\\.(jpg|jpeg|png|gif|eot|otf|webp|svg|ttf|woff|woff2|mp4|webm|wav|mp3|m4a|aac|oga|doc|docx|pdf|xls|xlsm)$": "<rootDir>/src/assetsTransformer.js",
      "\\.(css|less|scss)$": "<rootDir>/src/assetsTransformer.js"
    },
    "snapshotSerializers": [
      "enzyme-to-json/serializer"
    ]
  },
  "jestSonar": {
    "reportPath": "reports",
    "reportFile": "test-reporter.xml",
    "indent": 4
  },
  "jest-junit": {
    "output": "./coverage/tests-report.xml"
  },
  "browserslist": [
    "> 0.1%"
  ],
  "lint-staged": {
    "*.{js,ts,tsx,css,json,md}": [
      "yarn format:write",
      "git add"
    ],
    "../minespace-web/**/*.{js,ts,tsx,css,json,md}": [
      "yarn format:write",
      "git add"
    ],
    "*.js,*.ts,*.tsx": [
      "eslint --fix",
      "git add"
    ],
    "../minespace-web/**/*.{js,ts,tsx}": [
      "eslint --fix",
      "git add"
    ]
  },
  "lint-prepush": {
    "base": "develop",
    "tasks": {
      "*.{js,ts,tsx}": []
    }
  }
}
