  "scripts": {
    "serve": "cross-env HOST=0.0.0.0 PORT=3000 webpack-dev-server --hot --env development",
    "vfs": "webpack configtest ./webpack.config.ts",
    "build": "webpack --env production",
    "ci-test": "jest --env=jsdom --testResultsProcessor=jest-sonar-reporter --coverage",
    "lint": "eslint ./src",
    "lint:file": "eslint",
    "format:check": "prettier  --config ../../.prettierrc.json src/*.{js,ts,jsx,tsx}",
    "format:check-all": "prettier --config ../../.prettierrc.json src/*.{js,ts,jsx,tsx} && prettier  --config ../../.prettierrc.json src/*.js src/**/*.{js,ts,jsx,tsx} && prettier  --config ../../.prettierrc.json src/*.js src/**/**/*.{js,ts,jsx,tsx}",
    "format:write": "prettier --config ../../.prettierrc.json --write",
    "format:write-all": "prettier --config ../../.prettierrc.json src/*.{js,ts,jsx,tsx} --write && prettier --config ../../.prettierrc.json src/**/*.{js,ts,jsx,tsx} --write && prettier --config ../../.prettierrc.json src/**/**/*.{js,ts,jsx,tsx} --write",
    "test": "jest --env=jsdom ",
    "test:coverage": "jest --env=jsdom --runInBand src -- --testResultsProcessor=jest-junit --coverage",
    "test:updateAll": "jest --env=jsdom -u --",
    "test:watch": "jest --env=jsdom --runInBand -- src --watch",
    "test:clear": "jest --clearCache"
  },
  "jestSonar": {
    "reportPath": "reports",
    "reportFile": "test-reporter.xml",
    "indent": 4
  },
  "jest-junit": {
    "output": "./coverage/tests-report.xml"
  },
  "browserslist": [
    "> 0.1%"
  ]
}
