exports[`VariableConditionMenu renders properly 1`] = `
<div
  className="condition-menu-container"
>
  <h4>
    Condition Data Variables
    <CoreTooltip
      iconColor="white"
      title="Hover your mouse over the menus until you find the variable data you'd like to enter. Put it into your edited Permit Condition by clicking on it. This will populate the edited condition with a variable in the Draft permit screen. The Data from the Application tab will show up correctly in the PDF Draft permit in place of the variable. Please ensure all variable data fields you select have the correct information in the Application tab before adding these fields to your draft permit."
    />
  </h4>
  <Menu
    className="variable-menu"
    mode="vertical"
    onClick={[Function]}
  >
    <SubMenu
      key="mine"
      title="Mine"
    >
      <MenuItem
        className="variable-item"
        key="{mine_name}"
      >
        Mine Name
      </MenuItem>
      <MenuItem
        className="variable-item"
        key="{mine_no}"
      >
        Mine Number
      </MenuItem>
    </SubMenu>
    <SubMenu
      key="now"
      title="Notice of Work"
    >
      <MenuItem
        className="variable-item"
        key="{proposed_annual_maximum_tonnage}"
      >
        Proposed Annual Tonnage
      </MenuItem>
      <MenuItem
        className="variable-item"
        key="{application_type}"
      >
        Application Type
      </MenuItem>
      <MenuItem
        className="variable-item"
        key="{application_dated}"
      >
        Application Dated
      </MenuItem>
      <MenuItem
        className="variable-item"
        key="{application_last_updated_date}"
      >
        Application Last Updated
      </MenuItem>
    </SubMenu>
    <SubMenu
      key="draft"
      title="Draft Permit"
    >
      <MenuItem
        className="variable-item"
        key="{issue_date}"
      >
        Issue Date
      </MenuItem>
      <MenuItem
        className="variable-item"
        key="{authorization_end_date}"
      >
        Authorization End Date
      </MenuItem>
      <MenuItem
        className="variable-item"
        key="{permit_no}"
      >
        Permit Number
      </MenuItem>
    </SubMenu>
    <SubMenu
      key="sec"
      title="Security"
    >
      <MenuItem
        className="variable-item"
        key="{total_liability}"
      >
        Total Liability
      </MenuItem>
      <MenuItem
        className="variable-item"
        key="{liability_adjustment}"
      >
        Assessed Liability Adjustment
      </MenuItem>
      <MenuItem
        className="variable-item"
        key="{security_received_date}"
      >
        Security Received Date
      </MenuItem>
    </SubMenu>
    <SubMenu
      key="email"
      title="Emails"
    >
      <MenuItem
        className="variable-item"
        key="{major_mine_inbox}"
      >
        Major Mine Inbox
      </MenuItem>
      <MenuItem
        className="variable-item"
        key="{regional_mine_inbox}"
      >
        Regional Mine Inbox
        <CoreTooltip
          title="Defaults to the region associated with the Mine."
        />
      </MenuItem>
    </SubMenu>
  </Menu>
</div>
`;
