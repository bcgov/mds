{
  "parser": "babel-eslint",
  "env": {
    "browser": true,
    "node": true,
    "jest/globals": true,
    "es6": true
  },
  "plugins": ["jest"],
  "extends": [
    "eslint:recommended",
    "plugin:react/recommended",
    "airbnb-base",
    "prettier"
  ],
  "rules": {
    "import/no-unresolved": "off",
    "no-unused-vars": 1,
    "no-console": 1,
    "no-case-declarations": 0,
    "react/prop-types": 1,
    "react/display-name": 0,
    "react/no-deprecated": 0,
    "semicolon": [true, "always"]
  }
}
